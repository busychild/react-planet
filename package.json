{
	"name": "react-planet",
	"version": "1.0.1",
	"description": "A react lib for building circular menus in a very easy and handy way.",
	"author": "innFactory",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/innFactory/react-planet"
	},
	"keywords": [
		"react-planet",
		"planet",
		"planet-menu",
		"circular",
		"round",
		"circular-menu",
		"menu",
		"submenu"
	],
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"jsnext:main": "dist/index.es.js",
	"engines": {
		"node": ">=8",
		"npm": ">=5"
	},
	"scripts": {
		"test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
		"test:watch": "react-scripts-ts test --env=jsdom",
		"build": "rollup -c",
		"start": "start-storybook -p 9001 -c .storybook -s ./src/stories/public",
		"prepare": "npm run build",
		"predeploy": "npm install && npm run build",
		"deploy": "storybook-to-ghpages",
		"storybook": "npm start",
		"build-storybook": "build-storybook"
	},
	"dependencies": {
		"@emotion/core": "^11.0.0",
		"@emotion/react": "^11.10.0",
		"@emotion/styled": "^11.10.0",
		"@mui/icons-material": "^5.8.4",
		"@mui/material": "^5.10.0",
		"@mui/styles": "^5.9.3",
		"react-spring": "^9.5.2",
		"react-use-gesture": "^9.1.3",
		"use-resize-observer": "^9.0.2"
	},
	"peerDependencies": {
		"react": ">=18.2.0",
		"react-dom": ">=18.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@storybook/addon-actions": "^6.5.10",
		"@storybook/addon-links": "^6.5.10",
		"@storybook/addon-viewport": "^6.5.10",
		"@storybook/addon-controls": "^6.5.10",
		"@storybook/react": "^6.5.10",
		"@storybook/storybook-deployer": "^2.8.12",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.3.0",
		"@types/classnames": "2.3.1",
		"@types/jest": "^28.1.6",
		"@types/react": "18.0.17",
		"@types/react-dom": "18.0.6",
		"@types/react-motion": "0.0.33",
		"@types/react-virtualized": "9.21.21",
		"awesome-typescript-loader": "^5.2.1",
		"babel-loader": "^8.2.5",
		"jest": "^28.1.3",
		"prettier": "2.7.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-easy-crop": "4.4.2",
		"rollup": "^2.77.3",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-typescript2": "^0.32.1",
		"ts-jest": "^28.0.7",
		"typescript": "4.7.4"
	},
	"files": [
		"dist"
	]
}
